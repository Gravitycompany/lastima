-- Kill Aura (SOFT)
local SoftToggle = Main4Tab:CreateToggle({
   Name = "Kill Aura (SOFT)",
   CurrentValue = false,
   Flag = "KillAuraSoft",
   Callback = function(state)
      if state then
         getgenv().attackPlayer = true
         local player = game.Players.LocalPlayer
         local humanoidRootPart = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
         if humanoidRootPart then
            task.spawn(function()
               while getgenv().attackPlayer do
                  for i = 1, 25 do
                     game:GetService("ReplicatedStorage").Events.Punch:FireServer(0, 0.1, 1)
                     task.wait(0.05) -- üî• Antes 0.45 ‚Üí ahora 0.05 (mucho m√°s r√°pido)
                  end
                  task.wait(0.1) -- üî• Antes 0.5
               end
            end)
         end
      else
         getgenv().attackPlayer = false
      end
   end,
})

